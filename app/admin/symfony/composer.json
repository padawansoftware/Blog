{
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "../../core/symfony"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "blog/core": "^1.0",
        "friendsofsymfony/jsrouting-bundle": "^2.4",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/console": "^4.0",
        "symfony/dotenv": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "*",
        "symfony/framework-bundle": "^4.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "4.3.*",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "^4.0",
        "symfony/webpack-encore-bundle": "^1.0",
        "symfony/yaml": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Admin\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR% --symlink --relative": "symfony-cmd",
            "fos:js-routing:dump --format=json --target=%PUBLIC_DIR%/assets/js/fos_js_routes.json": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    },
    "require-dev": {
        "symfony/debug": "4.2.*",
        "symfony/maker-bundle": "^1.12",
        "symfony/profiler-pack": "^1.0"
    }
}
